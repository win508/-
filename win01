for i in range(1,101):
    print(i)
    print("ตัวแปรiมีค่า100รอบ")
for i in range(7,77,7):
    print(i)
    print("ตัวแปรiมีค่า10รอบ")
for i in range(120,1,-2):
    print(i)
    print("ตัวแปรiมีค่า10รอบ")
for i in range(2,18,3):
    print(i)
    print("ตัวแปรiมีค่า6รอบ")
for i in range(55,0,-11):
    print(i)
    print("ตัวแปรiมีค่า5รอบ")
for i in range(1,0):
    print(i)
    print("ตัวแปรiมีค่า0รอบ")
